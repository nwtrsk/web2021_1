<!Doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <title>Show message</title>
  </head>
  <body>
    <h2><%= data[0].センター %>の詳細</h2>
    <table>
      <% for(let row of data){ %>
        <tr><td>id</td><td><%= row.id %></td></tr>
        <tr><td>名前</td><td><%= row.名前 %></td></tr>
        <tr><td>期生</td><td><%= row.期生 %></td></tr>
        <tr><td>生年月日</td><td><%= row.生年月日 %></td></tr>
        <tr><td>出身</td><td><%= row.出身 %></td></tr>
        <tr><td>選抜数</td><td><%= row.選抜数 %></td></tr>
        <tr><td>参加シングル数</td><td><%= row.参加シングル数 %></td></tr>
       <% } %>
    </table>
  </body>
</html>