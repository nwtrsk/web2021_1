<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Show message</title>
</head>
<body>
  <h2>メンバーの情報</h2>
  <table border=1>
    <tr>
      <th>id</th>
      <th>名前</th>
      <th>期生</th>
      <th>生年月日</th>
      <th>出身</th>     
      <th>選抜数</th>
      <th>参加シングル数</th>
    </tr>
    <% for( let row of data ) { %>
      <tr>
        <td><%= row.id %></td>
        <td><%= row.名前 %></td>
        <td><%= row.期生 %></td>
        <td><%= row.生年月日 %></td>
        <td><%= row.出身 %></td>
        <td><%= row.選抜数 %></td>
        <td><%= row.参加シングル数 %></td>
      </tr>
    <% } %>
  </table>
  <a href="/public/top.html" class=button>戻る</a>
  <a href="/public/insert2.html" class=button>追加</a>
</body>
</html>