<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>売上割合情報</title>
  <link rel="stylesheet" href="/public/style.css">
</head>
<body>
  <h2 class="titlesin">シングルの情報</h2>
  <table border="1" class="sintable">
    <tr>
      <th>id</th>
      <th>シングル名</th>
      <th>発売日</th>
      <th>売上割合</th>
      <th>センター</th>
    </tr>
    <% for(let row of data) { %>
      <tr><td><%= row.id %></td>
      <td><%= row.シングル名 %></td>
      <td><%= row.発売日 %></td>
      <td><%= row.result %></td>
      <td><%= row.名前 %></td>
    <% } %>
  </table>
  <a href="/public/top.html" class=button>戻る</a>
</body>
</html>