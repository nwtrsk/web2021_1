<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>シングル情報</title>
  <link rel="stylesheet" href="/public/style.css">
</head>
<body>
  <h2 class="titlesin">シングルの情報</h2><br>
  <table border="1" class="sintable">
    <tr>
      <th>id</th>
      <th>シングル名</th>
      <th>発売日</th>
      <th>初日売上</th>
      <th>初週売上</th>
      <th>センター</th>
    </tr>
    <% for( let row of data ) { %>
      <tr>
        <td><%= row.id %></td>
        <td><%= row.シングル名 %></td>
        <td><%= row.発売日 %></td>
        <td><%= row.初日売上 %></td>
        <td><%= row.初週売上 %></td>
        <td><%= row.名前 %></td>
      </tr>
    <% } %>
  </table>
  <a href="/public/top.html">戻る</a>
  <a href="/public/insert.html">追加</a>
</body>
</html>