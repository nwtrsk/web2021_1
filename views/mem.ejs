<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>Show message</title>
  <link rel="stylesheet" href="/public/style.css">
</head>
<body>
  <h2 class="titlemem">メンバーの情報</h2>
  <table border="1" class="memtable">
    <tr>
      <th>id</th>
      <th>名前</th>
      <th>期生</th>
      <th>生年月日</th>
      <th>出身</th>
      <th>選抜割合</th>
    </tr>
    <% for(let row of data) { %>
      <tr><td><%= row.id %></td>
      <td><%= row.名前 %></td>
      <td><%= row.期生 %></td>
      <td><%= row.生年月日 %></td>
      <td><%= row.出身 %></td>
      <td><%= row.result %></td>
    <% } %>
  </table>
  <a href="/public/top.html" class=button>戻る</a>
</body>
</html>